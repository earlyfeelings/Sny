{
  "name": "nette/web-project",
  "description": "Nette: Standard Web Project",
  "keywords": ["nette"],
  "type": "project",
  "license": ["MIT", "BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
  "require": {
	"php": ">= 8.0",
	"nette/application": "^3.1",
	"nette/bootstrap": "^3.1",
	"nette/caching": "^3.1",
	"nette/database": "^3.1",
	"nette/di": "^3.0",
	"nette/finder": "^2.5",
	"nette/forms": "^3.1",
	"nette/http": "^3.1",
	"nette/mail": "^3.1",
	"nette/robot-loader": "^3.3",
	"nette/security": "^3.1",
	"nette/utils": "^3.2",
	"latte/latte": "^2.9",
	"tracy/tracy": "^2.8",

	"apalfrey/select2-bootstrap-5-theme": "*",
	"fortawesome/font-awesome": "*",
	"jackocnr/intl-tel-input": "*",
	"select2/select2": "*",
	"tacoberu/composer-assets": "2.*",
	"twbs/bootstrap": "^5.2"
  },
  "scripts": {
	"post-autoload-dump": [
	  "Taco\\ComposerScripts\\CopyAssetsToPublic::process"
	]
  },
  "config": {
	"assets-definition": "app/assets.json",
	"www-dir": "www",
	"allow-plugins": {
	  "symfony/thanks": true
	}
  },
  "require-dev": {
	"nette/tester": "^2.3",
	"symfony/thanks": "^1"
  },
  "autoload": {
	"psr-4": {
	  "App\\": "app"
	}
  },
  "minimum-stability": "stable"
}
